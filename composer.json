{
    "name": "thecodingmachine/tdbm-graphql",
    "description": "A class generator that will generate GraphQL types from your database schema (using youshido/graphql).",
    "license": "MIT",
    "authors": [
        {
            "name": "David NÃ©grier",
            "email": "d.negrier@thecodingmachine.com"
        }
    ],
    "require": {
        "youshido/graphql": "^1.4",
        "thecodingmachine/tdbm": "^5.0",
        "symfony/filesystem": "^2.0 || ^3.0",
        "psr/container": "^1.0",
        "thecodingmachine/graphql-controllers": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "TheCodingMachine\\Tdbm\\GraphQL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheCodingMachine\\Tdbm\\GraphQL\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev",
            "dev-2.x": "2.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
